GET         /procs                         controllers.RootController.procs()
GET         /procs/:namespace              controllers.RootController.procForNamespace(namespace: String)
GET         /procs/:namespace/:name        controllers.RootController.procEntry(namespace: String, name: String)
GET         /types                         controllers.RootController.types()
GET         /types/:id                     controllers.RootController.typeOf(id: Long)
POST        /call/:namespace/:name         controllers.RootController.call(namespace: String, name: String)

